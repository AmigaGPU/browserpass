{
  "manifest_version": 2,
  "name": "gopass",
  "description": "Chrome extension for zx2c4's pass",
  "version": "1.0",
  "author": "Danny van Kooten",
  "homepage_url": "https://github.com/dannyvankooten/gopass",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "content.html"
  },
  "permissions": [
    "activeTab",
    "nativeMessaging"
  ],
  "commands": {
   "_execute_browser_action": {
     "suggested_key": {
       "windows": "Ctrl+M",
       "mac": "Ctrl+M",
       "chromeos": "Ctrl+M",
       "linux": "Ctrl+M"
     }
   }
 }
}
